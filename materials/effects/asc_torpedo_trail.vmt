"UnlitGeneric"
{
	"$basetexture" "effects/asc_torpedo_trail"
	"$additive" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$translucent" "1"
	"$nocull" "1"
	"$ignorez" "0"
	"$nofog" "1"
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" "3.0"
			"texturescrollangle" "180"
		}
		"Sine"
		{
			"sineperiod" "0.8"
			"sinemin" "0.6"
			"sinemax" "1.0"
			"resultvar" "$alpha"
		}
	}
}
