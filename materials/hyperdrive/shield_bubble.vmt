"UnlitGeneric"
{
	"$basetexture" "hyperdrive/shield_bubble"
	"$bumpmap" "hyperdrive/shield_bubble_normal"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.2 0.4 1.0]"
	"$envmapfresnel" "1"
	"$translucent" "1"
	"$additive" "0"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$nocull" "1"
	"$alpha" "0.3"
	
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" "0.05"
			"texturescrollangle" "45"
		}
		"Sine"
		{
			"sineperiod" "4"
			"sinemin" "0.1"
			"sinemax" "0.5"
			"resultvar" "$alpha"
		}
	}
}
