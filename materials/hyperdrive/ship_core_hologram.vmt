"UnlitGeneric"
{
	"$basetexture" "hyperdrive/ship_core_hologram"
	"$additive" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$translucent" "1"
	"$nocull" "1"
	"$model" "1"
	
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" "0.1"
			"texturescrollangle" "90"
		}
		"Sine"
		{
			"sineperiod" "1.5"
			"sinemin" "0.2"
			"sinemax" "0.8"
			"resultvar" "$alpha"
		}
	}
}
