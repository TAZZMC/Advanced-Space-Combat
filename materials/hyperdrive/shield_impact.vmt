"UnlitGeneric"
{
	"$basetexture" "hyperdrive/shield_impact"
	"$additive" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$translucent" "1"
	"$nocull" "1"
	"$ignorez" "1"
	
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" "0.3"
			"texturescrollangle" "0"
		}
		"Sine"
		{
			"sineperiod" "0.5"
			"sinemin" "0.0"
			"sinemax" "1.0"
			"resultvar" "$alpha"
		}
	}
}
